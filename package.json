{
  "name": "greenshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier -w .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "stylelint": "stylelint '**/*.styled.tsx'",
    "stylelint:fix": "stylelint --fix '**/*.styled.tsx'",
    "server": "json-server -p 4000 db.json",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "yarn lint:fix",
      "yarn prettier"
    ],
    "*.styled.tsx": "yarn stylelint:fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "cookies-next": "^2.1.1",
    "firebase": "^11.0.1",
    "fslightbox-react": "^1.7.2",
    "i18next": "^23.11.5",
    "json-server": "^0.17.4",
    "next": "13.2.3",
    "next-i18next": "^15.3.0",
    "nouislider-react": "^3.4.1",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.43.5",
    "react-i18next": "^14.1.2",
    "react-select": "^5.8.0",
    "react-spring": "^9.7.1",
    "styled-components": "^5.3.8",
    "swiper": "^9.1.0",
    "swr": "^2.2.5",
    "use-debounce": "^10.0.1",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/fslightbox-react": "^1.7.0",
    "@types/json-server": "^0.14.7",
    "@types/node": "^18.14.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.21",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.3.3",
    "stylelint": "^15.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.13.0"
  }
}
